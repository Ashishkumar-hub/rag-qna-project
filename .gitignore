# Ignore Python bytecode and cache files
**/__pycache__/
*.pyc
*.pyo
*.pyd

# Ignore virtual environments
venv/
.env

# Ignore Alembic migration cache but keep actual migration scripts
alembic/__pycache__/
alembic/versions/__pycache__/

# Ignore test cache and artifacts
app/tests/__pycache__/
.pytest_cache/

# Ignore development and deployment-related files
configs/.env
configs/*.env

# Ignore IDE and editor-specific files
.vscode/

# Ignore test coverage reports
.coverage
.coverage.*
htmlcov/
